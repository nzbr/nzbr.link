{
  extends: "@parcel/config-default",
  transformers: {
    "*.hbs": [
      "parcel-transformer-handlebars"
    ],
    "*.{ts,tsx}": [
      "@parcel/transformer-typescript-tsc"
    ],
    "*.{ttf,woff,woff2}": [
      "parcel-transformer-fonts"
    ],
    "robots.txt": [
      "@parcel/transformer-raw"
    ],
  },
  "validators": {
    "*.{ts,tsx}": [
      "@parcel/validator-typescript"
    ]
  }
}
